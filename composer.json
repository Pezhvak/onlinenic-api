{
  "name": "pezhvak/onlinenic-api",
  "description": "A simple and useful onlinenic standalone package (with laravel support)",
  "type": "library",
  "require": {
    "illuminate/support": "~5.5",
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Pezhvak\\OnlinenicApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pezhvak\\OnlinenicApi\\OnlinenicServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Pezhvak",
      "email": "pezhvakimv@gmail.com"
    }
  ],
  "minimum-stability": "dev"
}
